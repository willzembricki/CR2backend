<p id="notice"><%= notice %></p>

<h1>States</h1>

<table>
  <thead>
    <tr>
      <th>Abbv</th>
      <th>Name</th>
      <th>Pop</th>
      <th>Asian pop</th>
      <th>Hawaiian</th>
      <th>Black</th>
      <th>American indian</th>
      <th>Unknown</th>
      <th>White</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @states.each do |state| %>
      <tr>
        <td><%= state.abbv %></td>
        <td><%= state.name %></td>
        <td><%= state.pop %></td>
        <td><%= state.asian_pop %></td>
        <td><%= state.hawaiian %></td>
        <td><%= state.black %></td>
        <td><%= state.american_indian %></td>
        <td><%= state.unknown %></td>
        <td><%= state.white %></td>
        <td><%= state.year %></td>
        <td><%= link_to 'Show', state %></td>
        <td><%= link_to 'Edit', edit_state_path(state) %></td>
        <td><%= link_to 'Destroy', state, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New State', new_state_path %>
